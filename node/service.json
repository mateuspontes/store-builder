{
  "memory": 256,
  "ttl": 10,
  "timeout": 2,
  "minReplicas": 2,
  "maxReplicas": 4,
  "routes": {
    "status": {
      "path": "/publishPage",
      "public": false
    },
    "install": {
      "path": "/checkPublishedApp",
      "public": false
    },
    "unpublish": {
      "path": "/unpublishPage",
      "public": false
    }
  },
  "events": {
    "buildStatusBuilderHub": {
      "sender": "vtex.builder-hub",
      "keys": ["build.status"]
    }
  }
}
